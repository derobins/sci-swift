#!/bin/bash
swift -A https://ngfobjtest.nersc.gov:5443/v3 -U $LOGNAME -K $OS_PASSWORD stat 
swift -A https://ngfobjtest.nersc.gov:5443/v3 -U $LOGNAME -K $OS_PASSWORD list
vr=$(swift -A https://ngfobjtest.nersc.gov:5443/v3 -U $LOGNAME -K $OS_PASSWORD list)  
echo $vr
for it in $vr
do
 swift -A https://ngfobjtest.nersc.gov:5443/v3 -U $LOGNAME -K $OS_PASSWORD delete $it
done
rm *.h5
rm *.hdf5
